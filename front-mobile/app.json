{
  "expo": {
    "name": "DS Delivery",
    "slug": "ds-delivery",
	"description": "App de entrega de pedidos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/appicon.png",
    "splash": {
      "image": "src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
	  "package": "com.nadilson.dsdelivery",
      "adaptiveIcon": {
        "foregroundImage": "src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "src/assets/appicon.png"
    }
  }
}
